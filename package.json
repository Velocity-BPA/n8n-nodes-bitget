{
  "name": "n8n-nodes-bitget",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for Bitget cryptocurrency exchange providing spot trading, futures trading, copy trading, and market data operations with full API v2 support.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-bitget.git"
  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-bitget#readme",
  "bugs": {
    "url": "https://github.com/Velocity-BPA/n8n-nodes-bitget/issues"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "bitget",
    "cryptocurrency",
    "crypto",
    "trading",
    "exchange",
    "futures",
    "spot",
    "copy-trading",
    "defi",
    "blockchain",
    "api",
    "automation"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "COMMERCIAL_LICENSE.md",
    "LICENSING_FAQ.md",
    "README.md"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/BitgetApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Bitget/Bitget.node.js",
      "dist/nodes/Bitget/BitgetTrigger.node.js"
    ]
  },
  "scripts": {
    "build": "tsc && npm run copy:icons",
    "copy:icons": "cp -r nodes/Bitget/*.svg dist/nodes/Bitget/ 2>/dev/null || true",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "crypto-js": "^4.2.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "n8n-workflow": "*",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
